  "scripts": {
    "build": "npm run clean && npm run copy & webpack --env=dist --profile --json > compilation-stats.json",
    "clean": "rimraf ./client/dist/*",
    "copy": "copyfiles -f ./client/src/index.html ./client/src/favicon.ico ./client/dist",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "eslint ./client/src",
    "lint:server": "eslint ./server",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "seed-dev": "node seed_rules.js --env=dev",
    "seed-dist": "node seed_rules.js --env=dist",
    "serve": "nodemon -w ./cfg -w ./server server/devServer.js",
    "serve:dist": "node server/index.js",
    "server": "nodemon -w ./cfg -w ./server server/index.js ",
    "client": "node server/justClient.js ",
    "start": "nodemon -w ./cfg -w ./server server/devServer.js",
    "test": "npm run test:server && npm run test:client",
    "test:client": "karma start",
    "test:client:watch": "karma start --autoWatch=true --singleRun=false",
    "test:server": "mocha test/test_server"
  }
